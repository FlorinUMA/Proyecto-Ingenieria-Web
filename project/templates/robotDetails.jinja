{# Makes use of base.jinja #}
{% extends 'base.jinja' %}

{% block head %}
{# Adds the title, stylesheet links and favicon #}

<title>Robots</title>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

{% endblock %}

{%block navbar%}
<div class="collapse navbar-collapse" id="menuItems">
    <ul class="navbar-nav me-auto">
        <li class="nav-item">
            <a href="/tecnico" class="nav-link">Volver a t√©cnico</a>
        </li>
    </ul>
</div>
{% endblock %}

{% block content%}

<div class="container">

    <h2>Tasks</h2>
          
    <table class="table tasks">
      <button OnClick="location.href='task-editor'">Edit</button>
      <thead class="thead-dark">
        <tr>
          <th scope="col">ID Task</th>
        </tr>
      </thead>
      <tbody>
            {% for i in row %}
              <tr>
                <td>{{i[0]}}</td>
              </tr>
            {% endfor %}
        

      </tbody>
    </table>
  </div>

{% endblock %}